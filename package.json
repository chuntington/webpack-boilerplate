{
	"name": "webpack-boilerplate",
	"version": "0.1.1",
	"description": "A simple webpack setup for generating static websites.",
	"scripts": {
		"build": "node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
		"lint": "node_modules/eslint/bin/eslint.js --ext js,vue ./src/assets/js/* -c .eslintrc.js",
		"watch": "node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=webpack.config.js"
	},
	"author": "Cameron Huntington",
	"license": "MIT",
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.1",
		"compression-webpack-plugin": "^1.1.7",
		"css-loader": "^0.28.9",
		"dotenv": "^5.0.0",
		"ejs-compiled-loader": "^1.1.0",
		"eslint": "^4.18.0",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-html": "^4.0.2",
		"extract-text-webpack-plugin": "^3.0.2",
		"glob": "^7.1.2",
		"html-webpack-inline-source-plugin": "0.0.9",
		"html-webpack-plugin": "^2.30.1",
		"node-sass": "^4.7.2",
		"optimize-css-assets-webpack-plugin": "^3.2.0",
		"raw-loader": "^0.5.1",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.20.2",
		"vue-loader": "^14.1.1",
		"vue-template-compiler": "^2.5.13",
		"webpack": "^3.11.0"
	},
	"dependencies": {
		"axios": "^0.17.1",
		"bootstrap": "^4.0.0",
		"date-fns": "^1.29.0",
		"jquery": "^3.3.1",
		"popper.js": "^1.12.9",
		"underscore": "^1.8.3",
		"vue": "^2.5.13"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chuntington/webpack-boilerplate.git"
	}
}
