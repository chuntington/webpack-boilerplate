{
	"name": "webpack-boilerplate",
	"version": "0.1.6",
	"description": "A simple webpack setup for generating static websites.",
	"scripts": {
		"build": "node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
		"lint": "node_modules/eslint/bin/eslint.js --ext js,vue ./src/assets/js/* -c .eslintrc.js",
		"watch": "node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=webpack.config.js"
	},
	"author": "Cameron Huntington",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.1.0",
		"@babel/preset-env": "^7.1.0",
		"babel-loader": "^8.0.2",
		"compression-webpack-plugin": "^1.1.12",
		"css-loader": "^1.0.0",
		"dotenv": "^6.0.0",
		"ejs-compiled-loader": "^1.1.0",
		"eslint": "^5.6.0",
		"eslint-loader": "^2.1.1",
		"eslint-plugin-html": "^4.0.6",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^2.0.0",
		"glob": "^7.1.3",
		"html-webpack-inline-source-plugin": "0.0.10",
		"html-webpack-plugin": "^3.2.0",
		"node-sass": "^4.9.3",
		"optimize-css-assets-webpack-plugin": "^3.2.0",
		"raw-loader": "^0.5.1",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.0",
		"uglifyjs-webpack-plugin": "^1.3.0",
		"vue-loader": "^15.4.2",
		"vue-template-compiler": "^2.5.17",
		"webpack": "^3.12.0"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"bootstrap": "^4.1.3",
		"date-fns": "^1.29.0",
		"jquery": "^3.3.1",
		"popper.js": "^1.14.4",
		"underscore": "^1.9.1",
		"vue": "^2.5.17"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chuntington/webpack-boilerplate.git"
	}
}
